# Ignore node_modules and build artifacts
node_modules/
dist/
*.tsbuildinfo

# Compiled output in src (should only be in dist/)
src/**/*.js
src/**/*.d.ts
src/**/*.d.ts.map

# IDE and editor files
.idea/
.vscode/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Test coverage
coverage/
*.lcov

# npm debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
*.log

# Environment files
.env
.env.local
.env.*.local

# Temporary files
*.tmp
*.temp

# Package manager files
.npmrc
.yarnrc
.pnpm-store/

# TypeScript cache
*.tsbuildinfo

# Auto-generated files (run 'npm run generate' to recreate)
src/generated/
src/constants.ts

# Allow bin folder for CLI scripts (override global gitignore)
!bin/

# Local samples (not part of the package)
samples/
example_cetin/
temp
test-results/
